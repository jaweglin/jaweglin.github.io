<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Jenzfolio2.0</title>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />

  <link rel="stylesheet" href="dest/main.min.css">
</head>
<body class="frontpage">
	{% include 'sections/header.njk' %}
	{% include 'partials/navigation.njk' %}

	<main class="page-wrap">
		<div id="menu-toggle">
			<div class="hamburger">
				<span></span>
				<span></span>
				<span></span>
			</div>
			<div class="cross">
				<span></span>
				<span></span>
				<span></span>
			</div>
		</div>

		{% block content %} {% endblock %}

	</main>
	<script src="dest/jquery.min.js" type="text/javascript"></script>
	<script src="dest/jquery.throwable.js" type="text/javascript"></script>
  <script src="dest/main.min.js"></script>
	<script>$(document).ready(function () {
		if ( $(window).width() > 1200) {
				$(".th").throwable({
          gravity:{x:0,y:5},
          autostart:true
        });
		}
	});</script>
	</script>
</body>
</html>
